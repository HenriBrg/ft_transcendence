<p id="notice"><%= notice %></p>

<h1>Room Admins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_admins.each do |room_admin| %>
      <tr>
        <td><%= link_to 'Show', room_admin %></td>
        <td><%= link_to 'Edit', edit_room_admin_path(room_admin) %></td>
        <td><%= link_to 'Destroy', room_admin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room Admin', new_room_admin_path %>
