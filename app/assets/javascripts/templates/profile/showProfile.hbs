<div class="margin25Div">


	
{{#if user}}

<h3>
	<u>
{{user.nickname}}

	</u>

</h3>
<br>
<br>
<img src="{{user.image}}" style="max-height: 200px;">
<br>
<h3>
	<br>
	<u>
	Guild

	</u>
	</h3>
	{{#if guild}}
	<br>
	<a class="btn btn-success" href="#guilds/{{guild.id}}">{{guild.name}}</a>
	{{else}}
	This user has no guild
	{{/if}}
<br>
<br>
{{else}}
<h1>This user does not seem to exist</h1>
<br>
{{/if}}
<br>
<br>
<a class="btn btn-primary" href="#profiles">Users list</a>
<br>
<br>
<a class="btn btn-info" href="#">Home</a>

</div>
